<script>

  window.onresize = ()=> {
    let pagination_count = 3;
    let pagination_total = 8;
    if (window.matchMedia('screen and (max-width: 768px)').matches) {
          pagination_count = 1;
          pagination_total = 4;
          paginate(pagination_count,pagination_total);
    }
    paginate(pagination_count,pagination_total);
  }
 $(function () {
   
       let pagination_count = 3;
       let pagination_total = 8;
       if (window.matchMedia('screen and (max-width: 768px)').matches) {
          pagination_count = 1;
          pagination_total = 4;
       }
       paginate(pagination_count,pagination_total);

  });

  function paginate(pagination_count,pagination_total){
        let container = $('#pagination');
        let cert_pagination =  $('#cert-pagination');
        container.pagination({
            dataSource: '/fetch-projects',
            locator: 'data',
            totalNumber: pagination_total,
            pageSize:  pagination_count,
            hideWhenLessThanOnePage: true,

            callback: function (obj, pagination) {
              var dataHtml = ``;
              console.log(pagination)
              $.each(obj, function (index, item) {
                    console.log(item);
                    dataHtml += `
                                    <div class="col-md-4">
                                      <div class="work-box">
                                        <a href="`+item.image_url+`" data-gallery="portfolioGallery" class="portfolio-lightbox">
                                          <div class="work-img">
                                            <img src="`+item.image_url+`" alt="`+item.title+`" class="img-fluid">
                                          </div>
                                        </a>
                                        <div class="work-content">
                                          <div class="row">
                                            <div class="col-sm-8">
                                              <h2 class="w-title">`+item.title+`</h2>
                                              <div class="w-more">
                                                <span class="w-ctegory">Machine Learning/ Web Application</span> <br><span class="w-date">`+item.date+`</span>
                                              </div>
                                            </div>
                                            <div class="col-sm-4">
                                                <a class="btn btn-success" href="`+item.url+`">Info</a>
                                            </div>
                                          </div>
                                        </div>
                                      </div>
                                    </div>`;
                });
                $('#data-container').html(dataHtml);
            }
        });

        cert_pagination.pagination({
            dataSource: '/fetch-certificates',
            locator: 'data',
            totalNumber: pagination_total,
            pageSize: pagination_count,
            hideWhenLessThanOnePage: true,
            callback: function (obj, pagination) {
              var dataHtml = ``;
              $.each(obj, function (index, item) {
                    dataHtml += `
                              <div class="col-md-4">
                                <div class="card card-blog">
                                  <div class="card-img">
                                    <a href="`+item.image_url+`" data-gallery="portfolioGallery" class="portfolio-lightbox">
                                      <div class="work-img">
                                        <img src=" `+item.image_url+`" alt="`+item.title+`" class="img-fluid">
                                      </div>
                                    </a>
                                  </div>
                                  <div class="card-body">
                                    <div class="card-category-box">
                                      <div class="card-category">
                                        <h6 class="category">`+item.type+`</h6>
                                      </div>
                                    </div>
                                    <h3 class="card-title"><a href="blog-single.html">`+item.title+`</a></h3>
                                    <p class="card-description">
                                      `+item.text+`
                                    </p>
                                  </div>
                                  <div class="card-footer">
                                    <div class="post-author">
                                      <a href="#">
                                        <img src="`+item.logo+`" alt="" class="avatar rounded-circle">
                                        <span class="author">`+item.source+`</span>
                                      </a>
                                    </div>
                                    <div class="post-date">
                                      <span class="bi bi-clock"></span>`+item.date+`
                                    </div>
                                  </div>
                                </div>
                              </div>
                    `;
                });
                $('#cert-container').html(dataHtml);
            }
        });
  }

  </script>
